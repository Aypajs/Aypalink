  "engines": {
    "node": ">=16.0.0"
  },
  "homepage": "https://github.com/Aypajs/Aypalink",
  "dependencies": {
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
